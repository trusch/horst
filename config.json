{
  "twitter": {
    "image": "trusch/horst/twittersource",
    "config": {
      "consumerKey": "4D6U7dJo1LemnP2qgoAYyHQjy",
      "consumerSecret": "qC9lzcKHvGQ9PAsmQNdTSWcLwXINllONWoTi2H5YpTB59qZdzj",
      "accessToken": "768081588655681536-rrJ2mRnycXtN9PvTFdJq3tC6g8UnmKb",
      "accessSecret": "TtvYLUadroMiiQsZhXiPhi7fVB6QRRIglv6LKn3qoeoKD",
      "track": ["a","the","i","you"]
    },
    "outputs": {
      "out": "http://projector"
    }
  },

  "projector": {
    "image": "trusch/horst/projector",
    "config": {
      "projection": "@text"
    },
    "outputs": {
      "out": "http://sanitizer"
    }
  },

  "sanitizer": {
    "image": "trusch/horst/textsanitizer",
    "config": {
      "toLower": true,
      "cutset": "@-!?,()[]{}.:"
    },
    "outputs": {
      "out": "http://splitter"
    }
  },

  "splitter": {
    "image": "trusch/horst/textsplitter",
    "outputs": {
      "out": "http://filter"
    }
  },

  "filter": {
    "image": "trusch/horst/textfilter",
    "config": {
      "regexp": "^#"
    },
    "outputs": {
      "out": "http://histogram"
    }
  },

  "histogram": {
    "image": "trusch/horst/texthistogram",
    "config": {
      "decay": 0.999,
      "min": 0.3,
      "minEmit": 3,
      "relative": true,
      "emitEvery": 50
    },
    "outputs": {
      "out": "http://logger/histogram"
    }
  },

  "logger": {
    "image": "trusch/horst/logger",
    "outputs": {},
    "config": {}
  }
}
